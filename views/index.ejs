<head>
  <meta charset="utf-8">
  <title>Youtube Sync</title>
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/semantic-ui@2.4.2/dist/semantic.css">
  <link rel="stylesheet" href="/css/main.css">
</head>

<div class="pushable">
  <div id="title" class="ui inverted vertical masthead center aligned segment">
    <h1 id="header">Youtube Sync</h1>
  </div>

  <div class="pusher">
    <div class="ui vertical masthead center aligned segment">
      <form action="/" method="POST">
        <div id="inputForm" class="ui input focus">
          <input
            required
            id="roomNameInput"
            type="text"
            name="roomName"
            placeholder="Enter room name"
          />
        </div>
        <div id="createRoomButtonDiv">
          <button id="createRoomButton" class="ui primary button">
            Create/Join Room
          </button>
        </div>
      </form>
    </div>
  </div>
</div>

<script
  src="https://code.jquery.com/jquery-3.4.1.min.js"
  integrity="sha256-CSXorXvZcTkaix6Yvo6HppcZGetbYMGWSFlBw8HfCJo="
  crossorigin="anonymous"
></script>